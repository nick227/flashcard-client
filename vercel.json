{
    "headers": [{
            "source": "/assets/(.*)\\.js",
            "headers": [{
                "key": "Content-Type",
                "value": "application/javascript"
            }]
        },
        {
            "source": "/(.*)\\.js",
            "headers": [{
                "key": "Content-Type",
                "value": "application/javascript"
            }]
        }
    ],
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/index.html",
        "has": [{
            "type": "header",
            "key": "Accept",
            "value": "(?i)text/html"
        }],
        "missing": [{
            "type": "file"
        }]
    }]
}