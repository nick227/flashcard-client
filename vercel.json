{
    "headers": [{
            "source": "/assets/(.*)\\.js",
            "headers": [{
                "key": "Content-Type",
                "value": "application/javascript"
            }]
        },
        {
            "source": "/(.*)\\.js",
            "headers": [{
                "key": "Content-Type",
                "value": "application/javascript"
            }]
        }
    ],
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/index.html"
    }],
    "cleanUrls": true,
    "trailingSlash": false
}