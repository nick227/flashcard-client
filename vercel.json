{
    "headers": [{
        "source": "/assets/(.*)\\.js",
        "headers": [{
            "key": "Content-Type",
            "value": "application/javascript"
        }]
    }],
    "rewrites": [{
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}