{
    "headers": [{
            "source": "/assets/(.*)\\.js",
            "headers": [{
                "key": "Content-Type",
                "value": "application/javascript"
            }]
        },
        {
            "source": "/(.*)\\.js",
            "headers": [{
                "key": "Content-Type",
                "value": "application/javascript"
            }]
        }
    ],
    "rewrites": [{
        "source": "/((?!api/|assets/|.*\\.(js|css|png|jpg|jpeg|svg|ico|webp|json|txt|woff2?|ttf|map)$).*)",
        "destination": "/index.html"
    }]
}